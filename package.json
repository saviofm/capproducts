{
    "name": "capproducts",
    "version": "1.0.0",
    "description": "CAP project for appgyver",
    "repository": "",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap-cloud-sdk/http-client": "^2.12.1",
        "@sap/cds": "6.3.2",
        "@sap/cds-dk": "6.3.2",
        "@sap/cds-odata-v2-adapter-proxy": "1.9.16",
        "@sap/textbundle": "4.1.0",
        "@sap/xsenv": "3.4.0",
        "@sap/xssec": "3.2.14",
        "cds-swagger-ui-express": "^0.5.0",
        "@types/node-fetch": "^2.6.1",
        "aws-sdk": "^2.1051.0",
        "cors": "^2.8.5",
        "express": "^4",
        "hdb": "0.19.1",
        "passport": "^0.5.2"
    },
    "devDependencies": {
        "@sap/cds-compiler": "3.4.4",
        "@sap/eslint-plugin-cds": "2.3.3",
        "@sap/eslint-plugin-ui5-jsdocs": "^2.0.5",
        "@sap/hdi-deploy": "4.4.1",
        "@sap/ux-specification": "^1.102.14",
        "@sap/ux-ui5-tooling": "^1.5.5",
        "@sapui5/ts-types": "^1.92.2",
        "eslint": "^7.32.0"
    },
    "engines": {
        "node": "16.x.x",
        "npm": "8.x.x"
    },
    "scripts": {
        "start": "cds run",
        "undeploy": "cf undeploy capproducts --delete-services --delete-service-keys"
    },
    "eslintConfig": {
        "extends": "eslint:recommended",
        "env": {
            "es2020": true,
            "node": true,
            "jest": true,
            "mocha": true
        },
        "globals": {
            "SELECT": true,
            "INSERT": true,
            "UPDATE": true,
            "DELETE": true,
            "CREATE": true,
            "DROP": true,
            "CDL": true,
            "CQL": true,
            "CXL": true,
            "cds": true
        },
        "rules": {
            "no-console": "off",
            "require-atomic-updates": "off"
        }
    },
    "cds": {
        "i18n": {
            "folders": [
                "_i18n",
                "i18n"
            ]
        },
        "requires": {
            "db": {
                "model": [
                    "db",
                    "srv"
                ],
                "kind": "hana",
                "pool": {
                    "acquireTimeoutMillis": 600000,
                    "min": 0,
                    "max": 600000
                }
            },
            "uaa": {
                "kind": "xsuaa"
            }
        },
        "hana": {
            "deploy-format": "hdbtable"
        }
    },
    "sapux": [
        "app/products"
    ]
}
