{
    "name": "capproducts",
    "version": "1.0.0",
    "description": "CAP project for appgyver",
    "repository": "",
    "license": "UNLICENSED",
    "private": true,
    "dependencies": {
        "@sap/cds": "5.8.0",
        "@sap/cds-dk": "^4.8.0",
        "@sap/cds-odata-v2-adapter-proxy": "1.8.6",
        "@sap/textbundle": "4.1.0",
        "@sap/xsenv": "3.2.0",
        "@sap/xssec": "3.2.13",
        "@types/node-fetch": "^2.6.1",
        "aws-sdk": "^2.1051.0",
        "cds-swagger-ui-express": "^0.3.0",
        "convert-excel-to-json": "^1.7.0",
        "cors": "^2.8.5",
        "express": "^4",
        "hdb": "0.19.1",
        "mbt": "^1.2.7",
        "passport": "^0.5.2",
        "xlsx": "^0.17.4"
    },
    "devDependencies": {
        "@sap/cds-compiler": "2.12.0",
        "@sap/eslint-plugin-ui5-jsdocs": "^2.0.5",
        "@sap/hdi-deploy": "^4.2.3",
        "@sap/ux-specification": "^1.96.2",
        "@sapui5/ts-types": "^1.92.2",
        "eslint": "^7.32.0",
        "sqlite3": "^5.0.2"
    },
    "engines": {
        "node": "16.13.1",
        "npm": "8.4.0"
    },
    "scripts": {
        "start": "cds run",
        "undeploy": "cf undeploy capproducts --delete-services --delete-service-keys"
    },
    "eslintConfig": {
        "extends": "eslint:recommended",
        "env": {
            "es2020": true,
            "node": true,
            "jest": true,
            "mocha": true
        },
        "globals": {
            "SELECT": true,
            "INSERT": true,
            "UPDATE": true,
            "DELETE": true,
            "CREATE": true,
            "DROP": true,
            "CDL": true,
            "CQL": true,
            "CXL": true,
            "cds": true
        },
        "rules": {
            "no-console": "off",
            "require-atomic-updates": "off"
        }
    },
    "cds": {
        "i18n": {
            "folders": [
                "_i18n",
                "i18n"
            ]
        },
        "requires": {
            "db": {
                "model": [
                    "db",
                    "srv"
                ],
                "kind": "hana",
                "pool": {
                    "acquireTimeoutMillis": 600000,
                    "min": 0,
                    "max": 600000
                }
            },
            "uaa": {
                "kind": "xsuaa"
            }
        },
        "hana": {
            "deploy-format": "hdbtable"
        }
    },
    "sapux": [
        "app/products"
    ]
}
